{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "width: fit-content"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "iframe[loading]"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}